@using Application.DTO;

@model UserDTO
@{
    ViewBag.Title = "Index";
}

<h2>Users</h2>

<div class="users">
    <div>
        <span>Name: @Model.FullName</span>
    </div>
    <div>
        <span>Status: @Model.Status</span>
    </div>
    <div>
        <span>Image: @Model.Image</span>
    </div>
    <div>
        <span>Follower: @Model.Follower</span>
    </div>
    <div>
        <span>Following: @Model.Following</span>
    </div>
</div>
